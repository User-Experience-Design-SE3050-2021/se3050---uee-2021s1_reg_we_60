<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1C1B1B"
    tools:context=".MyAccount">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="417dp"
        android:layout_height="425dp"
        android:background="@drawable/vie_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">
        b

        <TextView
            android:id="@+id/gflockName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-light"
            android:text="GFLOCK"
            android:textColor="@color/black"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.529"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.042" />

        <Button
            android:id="@+id/profilebtn"
            android:layout_width="384dp"
            android:layout_height="32dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/profilebtn"
            android:text="     Profile"
            android:textAlignment="viewStart"
            app:layout_constraintBottom_toTopOf="@+id/shippingBtn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.227" />

        <Button
            android:id="@+id/shippingBtn"
            android:layout_width="384dp"
            android:layout_height="32dp"
            android:layout_marginBottom="28dp"
            android:background="@drawable/profilebtn"
            android:text="     SHIPPING ADDRESS"
            android:textAlignment="viewStart"
            app:layout_constraintBottom_toTopOf="@+id/interestsBtn"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/interestsBtn"
            android:layout_width="384dp"
            android:layout_height="32dp"
            android:layout_marginBottom="112dp"
            android:background="@drawable/profilebtn"
            android:text="     MY INTERESTS"
            android:textAlignment="viewStart"
            app:layout_constraintBottom_toTopOf="@+id/button5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/interestsBtn2"
            android:layout_width="384dp"
            android:layout_height="32dp"
            android:layout_marginBottom="32dp"
            android:background="@drawable/profilebtn"
            android:text="     MY WALLET"
            android:textAlignment="viewStart"
            app:layout_constraintBottom_toTopOf="@+id/button5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/interestsBtn"
            app:layout_constraintVertical_bias="0.756" />

        <Button
            android:id="@+id/button5"
            android:layout_width="323dp"
            android:layout_height="44dp"
            android:background="@drawable/btn"
            android:backgroundTint="#1C1B1B"
            android:text="SIGN OUT"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.506"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.959" />

        <ImageView
            android:id="@+id/profileForward"
            android:layout_width="36dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/shippingBtn"
            app:layout_constraintEnd_toEndOf="@+id/profilebtn"
            app:layout_constraintHorizontal_bias="0.956"
            app:layout_constraintStart_toStartOf="@+id/profilebtn"
            app:layout_constraintTop_toBottomOf="@+id/gflockName"
            app:layout_constraintVertical_bias="0.677"
            app:srcCompat="@drawable/arrowf" />

        <ImageView
            android:id="@+id/shippingForward"
            android:layout_width="36dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/interestsBtn"
            app:layout_constraintEnd_toEndOf="@+id/shippingBtn"
            app:layout_constraintHorizontal_bias="0.958"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profilebtn"
            app:layout_constraintVertical_bias="0.452"
            app:srcCompat="@drawable/arrowf" />

        <ImageView
            android:id="@+id/interestsForward"
            android:layout_width="36dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/interestsBtn2"
            app:layout_constraintEnd_toEndOf="@+id/interestsBtn"
            app:layout_constraintHorizontal_bias="0.958"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/shippingBtn"
            app:layout_constraintVertical_bias="0.432"
            app:srcCompat="@drawable/arrowf" />

        <ImageView
            android:id="@+id/walletForward"
            android:layout_width="36dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toTopOf="@+id/button5"
            app:layout_constraintEnd_toEndOf="@+id/interestsBtn2"
            app:layout_constraintHorizontal_bias="0.958"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/interestsBtn"
            app:layout_constraintVertical_bias="0.463"
            app:srcCompat="@drawable/arrowf" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="204dp"
        android:layout_height="37dp"
        android:layout_marginStart="132dp"
        android:text="MY ACCOUNT"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/profileName"
        android:layout_width="204dp"
        android:layout_height="37dp"
        android:fontFamily="sans-serif-medium"
        android:text="HELLO BULNER"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.584"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/circularImageView"
        app:layout_constraintVertical_bias="0.727" />

    <ImageView
        android:id="@+id/loginBack"
        android:layout_width="37dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.072"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/next" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/circularImageView"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_gravity="center"
        android:src="@drawable/propic"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.361" />

</androidx.constraintlayout.widget.ConstraintLayout>